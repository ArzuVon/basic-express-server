{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jest": true,
    "es6": true
  },
  "globals": {
    "fail":true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
  },
  "rules": {
    "no-console": "off",
    "indent": [ "error", 2 ],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "comma-dangle": ["error", "always-multiline"],
    "semi": [ "error", "always" ],
    "no-unused-vars": [1, {"vars": "local", "args": "none"}]
        }
    }
}
